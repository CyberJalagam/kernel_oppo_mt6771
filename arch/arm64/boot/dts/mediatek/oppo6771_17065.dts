&spi3 {
    /* Novatek device tree node */
    status = "ok";
    #address-cells = <1>;
    #size-cells = <0>;
    touchscreen@0 {
        status = "okay";
        compatible = "oppo,tp_noflash";
        reg = <0>;
        chip-name = "OPPO_TP_NOFLASH";

        spi-max-frequency = <9600000>;	//4800000,9600000,15000000,19200000
        reset-gpio = <&pio 42 0x00>;
        interrupt-parent = <&pio>;
        interrupts = <1 IRQ_TYPE_EDGE_RISING 1 0>;
        irq-gpio = <&pio 1 0x2001>;
        lcd-reset-gpio = <&pio 45 0x00>;

        synaptics,irq-on-state = <0>;
        synaptics,reset-on-state = <0>;
        synaptics,reset-active-ms = <20>;
        synaptics,reset-delay-ms = <200>;
        synaptics,power-delay-ms = <200>;
        synaptics,spi-mode = <3>;
        synaptics,byte-delay-us = <0>;
        synaptics,block-delay-us = <0>;
        synaptics,ubl-max-freq = <1000000>;
        synaptics,ubl-byte-delay-us = <20>;
        synaptics,grip-darkzone-area = <0xF4 0x45>;
        synaptics,irq_trigger_hdl_support;

        touchpanel,max-num-support = <10>;
        touchpanel,tx-rx-num = <16 36>;
        touchpanel,panel-coords = <1080 2340>;
        touchpanel,display-coords = <1080 2340>;

        incell_screen;
        fw_edge_limit_support;
        black_gesture_support;
        charger_pump_support;
        headset_pump_support;
        //spurious_fingerprint_support;
        //lcd_trigger_fp_check;
        black_gesture_test_support;
        game_switch_support;
        noflash_support;
        lcd_trigger_load_tp_fw_support;
        smart_gesture_support;
        fw_update_app_support;
        pressure_report_support;
        freq_hop_simulate_support;

        /*
        *Virtual Key Config:
        *1:touchpanel,button-type: <virtualkey_type  supportted_button-map>
        *virtualkey_type:
        *Type 1: using board_properties
        *Type 2: using same IC (button zone&& touch zone are seprate)
        *Type 3: using diffrent IC (button zone&& touch zone are seprate)
        *Type 4: No need of virtual key process
        *supportted_button-map:select|menu|home|back
        *
        *2:touchpanel,button-map
        *type (X1,Y1)(X2,Y2)(X3,Y3)(Width_X,Hight_Y)--Only when button-type is 0
        */

        touchpanel,button-type = <4>;
    };
};
